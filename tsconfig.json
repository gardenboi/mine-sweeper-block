{
	"compilerOptions": {
	  "target": "ESNext",
	  "module": "ESNext",
	  "jsx": "react-jsx",
	  "strict": true,
	  "strictNullChecks": true,
	  "moduleResolution": "Node",
	  "resolveJsonModule": true,
	  "allowSyntheticDefaultImports":true,
	  "typeRoots": [
		"node_modules/@types",
		"@types"
	  ],
	  "paths": {
		"*" : ["./src/*.d.ts"]
	  }
	},
	"include": ["./src/*.tsx"]
  }